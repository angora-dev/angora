{
  "name": "@angora/fetch",
  "version": "0.0.9",
  "description": "Blazingly Fast Data Fetching ðŸ”¥",
  "homepage": "https://fetch.angora.dev",
  "repository": "angora-dev/angora",
  "bugs": "https://github.com/angora-dev/angora/issues",
  "author": {
    "name": "Mertin Dervish",
    "email": "mertindervish@gmail.com",
    "url": "https://mertindervish.com"
  },
  "files": ["dist", "lib/index.d.ts"],
  "types": "./lib/index.d.ts",
  "main": "./dist/main.js",
  "exports": {
    ".": {
      "import": "./dist/main/main.mjs",
      "require": "./dist/main/main.js",
      "default": "./dist/main/main.js"
    },
    "./next": {
      "import": "./dist/next/web.mjs",
      "require": "./dist/next/web.js",
      "default": "./dist/next/web.js"
    },
    "./next/with-angora-fetch": "./dist/with-angora-fetch/with-angora-fetch.js"
  },
  "license": "MIT",
  "scripts": {
    "lint": "TIMING=1 eslint \"**/*.ts*\"",
    "build:main": "vite --config main-vite.config.js build",
    "build:with-angora-fetch": "vite --config with-angora-fetch-vite.config.js build",
    "build:next": "vite --config next-vite.config.js build",
    "build": "concurrently \"npm:build:*\" && tsc -b",
    "dev:main": "vite --config main-vite.config.js build --watch",
    "dev:with-angora-fetch": "vite --config with-angora-fetch-vite.config.js build --watch",
    "dev:next": "vite --config next-vite.config.js build --watch",
    "dev:ts": "tsc -b --watch",
    "dev": "concurrently \"npm:dev:*\"",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@angora/tsconfig": "*",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "concurrently": "7.6.0",
    "eslint": "8.30.0",
    "eslint-config-angora": "*",
    "react": "18.2.0",
    "rollup-plugin-analyzer": "4.0.0",
    "typescript": "4.9.4",
    "webpack": "5.75.0",
    "vite": "4.0.3"
  },
  "dependencies": {
    "urlpattern-polyfill": "6.0.2"
  }
}
